@model CategoriesViewModel

<ul class="nav nav-tabs">
    @foreach (var c in Model.Categories)
    {
        <li class="@c.Selected">
            <a asp-controller="Home" asp-action="Category" asp-route-catName="@c.Name">@c.DisplayName</a>
        </li>
    }
</ul>

<ul class="thumbnails">
    @Html.Partial("FlowerList", Model.FlowerList)
</ul>
